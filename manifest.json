{
    "manifest_version": 3,
    "name": "Don't ask 4 Wi-Fi",
    "version": "1.0",
    "icons": {
        "16": "img/logo.png",
        "32": "img/logo.png",
        "48": "img/logo.png",
        "128": "img/logo.png"
    },
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": "img/logo.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://signup.ask4.com/fr/guest-signup*"
            ],
            "js": [
                "autofill.js"
            ],
            "all_frames": false,
            "match_origin_as_fallback": false,
            "match_about_blank": false,
            "run_at": "document_idle",
            "world": "ISOLATED"
        }
    ]
}